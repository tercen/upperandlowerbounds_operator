{
  "name": "Upper and Lower Bounds",
  "description": "Calculates an upper and lower bound value as a fraction of a given data point.",
  "tags": [
    "descriptive statistics"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/upperandlowerbounds_operator"
  ],
  "properties": [
    {
      "kind": "DoubleProperty",
      "name": "upper bound",
      "defaultValue": 1,
      "description": "upper value as a fraction"
    },
    {
      "kind": "DoubleProperty",
      "name": "lower bound",
      "defaultValue": -1,
      "description": "lower value as a fraction"
    },
    {
      "kind": "BooleanProperty",
      "name": "percentage",
      "defaultValue": true,
      "description": "A logical variable indicating whether to return the relative bounds as a percentage. If TRUE then it returns a percentage."
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [],
        "axis": [
          {
            "kind": "AxisSpec",
            "metaFactors": [
              {
                "kind": "MetaFactor",
                "name": "data",
                "type": "numeric",
                "description": "input data for the multiplication per cell",
                "ontologyMapping": "measurement",
                "crosstabMapping": "y",
                "cardinality": "1",
                "factors": []
              }
            ]
          }
        ]
      }
    ]
  }
}